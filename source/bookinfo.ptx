     \newcommand\YoungTableau[1]{
     \begin{tikzpicture}[scale=0.4,keepsize]
     \foreach \y [count=\yi] in {#1}
     \foreach \x in {1,...,\y}
     {
     \draw (\x,\yi) rectangle +(1,1);
     }
     \end{tikzpicture}
     }

