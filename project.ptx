<?xml version="1.0" encoding="utf-8"?>
<!-- (delete the above line to manage this file manually) -->
<!--
    This file, the project manifest, provides the overall configuration for
    your PreTeXt project. To edit the content of your document, open
    `source/main.ptx`. See
    https://pretextbook.org/doc/guide/html/processing-CLI.html#cli-project-manifest.
-->
<project ptx-version="2">
  <targets>
    <target 
      name="web" 
      format="html"
      publication="publication-html.ptx"/>
    <target 
      name="print" 
      format="pdf"
      publication="publication-latex.ptx"/>
    <target 
      name="print-latex" 
      format="latex"
      publication="publication-latex.ptx"
      output-filename="algebra.tex"
      xsl="custom-latex.xsl"/>
    <target 
      name="subset" 
      format="html"
      publication="publication-html.ptx">
      <stringparams debug.skip-knowls="yes" />
    </target>
  </targets>
</project>
